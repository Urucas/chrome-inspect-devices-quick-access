{
  "name": "Chrome inspect devices Quick Access",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Open Chrome Inspect Devices in a click",
  "homepage_url": "https://github.com/Urucas/chrome-inspect-devices-quick-access",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Chrome Inspect Devices"
  }
}
